{
    "name": "bigpoint/monolog-creator",
    "type" : "library",
    "description" : "These classes provide a simple factory for creating preconfigured monolog logger objects.",
    "homepage" : "https://github.com/Bigpoint/monolog-creator",
    "keywords" : ["logging", "log", "monolog"],
    "license" : "Apache-2.0",
    "authors": [
        {
            "name": "Peter Ahrens",
            "email": "pahrens@bigpoint.net"
        },
        {
            "name": "Andreas Schleifer",
            "email": "aschleifer@bigpoint.net"
        }
    ],
    "support" : {
        "issues" : "https://github.com/Bigpoint/monolog-creator/issues"
    },
    "require": {
        "php": ">=5.3.3",
        "monolog/monolog": "1.12.*",
        "predis/predis": "1.0.*"
    },
    "require-dev": {
        "phpunit/phpunit": "4.*"
    },
    "autoload": {
        "psr-0": {
            "MonologCreator\\": ["src/", "tests/"]
        }
    }
}
